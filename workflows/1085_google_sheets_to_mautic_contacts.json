{
  "id": "1085",
  "filename": "1085_google_sheets_to_mautic_contacts",
  "description": "Every 5 minutes, this workflow reads data from Google Sheets and automatically updates or adds contacts in Mautic, streamlining your marketing efforts.",
  "content": {
    "nodes": [
      {
        "name": "GS Read Data2",
        "type": "n8n-nodes-base.googleSheets",
        "position": [
          240,
          750
        ],
        "parameters": {
          "range": "Data!A:P",
          "options": {
            "valueRenderMode": "FORMATTED_VALUE"
          },
          "sheetId": "1jKYwPE9DMFOYf1AeDuTvQ3GSM2GqaEJhGYNoisxSLpM"
        },
        "credentials": {
          "googleApi": "n8n API"
        },
        "typeVersion": 1
      },
      {
        "name": "Mautic",
        "type": "n8n-nodes-base.mautic",
        "position": [
          450,
          750
        ],
        "parameters": {
          "email": "={{$node[\"GS Read Data2\"].json[\"email\"]}}",
          "options": {},
          "firstName": "={{$node[\"GS Read Data2\"].json[\"firstname\"]}}",
          "additionalFields": {
            "mobile": "={{$node[\"GS Read Data2\"].json[\"mobile\"]}}"
          }
        },
        "credentials": {
          "mauticApi": "MauticAPI"
        },
        "notesInFlow": false,
        "typeVersion": 1
      },
      {
        "name": "GS Read Data2",
        "type": "n8n-nodes-base.googleSheets",
        "position": [
          240,
          750
        ],
        "parameters": {
          "range": "Data!A:P",
          "options": {
            "valueRenderMode": "FORMATTED_VALUE"
          },
          "sheetId": "1jKYwPE9DMFOYf1AeDuTvQ3GSM2GqaEJhGYNoisxSLpM"
        },
        "credentials": {
          "googleApi": "n8n API"
        },
        "typeVersion": 1
      },
      {
        "name": "Cron",
        "type": "n8n-nodes-base.cron",
        "position": [
          40,
          750
        ],
        "parameters": {
          "triggerTimes": {
            "item": [
              {
                "mode": "everyX",
                "unit": "minutes",
                "value": 5
              }
            ]
          }
        },
        "typeVersion": 1
      }
    ],
    "connections": {
      "Cron": {
        "main": [
          [
            {
              "node": "GS Read Data2",
              "type": "main",
              "index": 0
            }
          ]
        ]
      },
      "GS Read Data2": {
        "main": [
          [
            {
              "node": "Mautic",
              "type": "main",
              "index": 0
            }
          ]
        ]
      }
    }
  },
  "title": "Automate data syncing from Google Sheets to Mautic contacts",
