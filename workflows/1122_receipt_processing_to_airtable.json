  ],
  "services": [
    "Airtable",
    "Edit Fields (Set)",
    "Mindee",
    "Webhook"
  ],
  "difficulty": "Intermediate",
  "triggers": [],
  "actions": [
    "Append",
    "Set Data"
  ],
  "setup_requirements": [
    "Airtable API Key",
    "Mindee API Key"
  ],
  "workflow_complexity": 4,
  "external_integrations_count": 4
}