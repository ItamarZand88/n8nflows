          200
        ],
        "parameters": {
          "options": {},
          "extractionValues": {
            "values": [
              {
                "key": "title",
                "cssSelector": "title"
              }
            ]
          }
        },
        "typeVersion": 1
      },
      {
        "name": "Add Link to Notion",
        "type": "n8n-nodes-base.notion",
        "position": [
          1250,
          200
        ],
        "parameters": {
