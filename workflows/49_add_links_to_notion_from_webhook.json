          "options": {},
          "httpMethod": "POST",
          "responseMode": "lastNode"
        },
        "typeVersion": 1
      },
      {
        "name": "HTTP Request",
        "type": "n8n-nodes-base.httpRequest",
        "position": [
          850,
          200
        ],
        "parameters": {
          "url": "=https://{{$json[\"body\"][\"data\"][\"options\"][0][\"value\"]}}",
          "options": {},
          "responseFormat": "string"
        },
        "typeVersion": 1
      },
      {
        "name": "Check type",
        "type": "n8n-nodes-base.if",
        "position": [
